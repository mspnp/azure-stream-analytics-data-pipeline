    {
      "name": "[parameters('outputCosmosDatabaseAccount')]",
      "type": "Microsoft.DocumentDB/databaseAccounts",
      "apiVersion": "2015-04-08",
      "location": "[resourceGroup().location]",
      "kind": "GlobalDocumentDB",
      "properties": {
        "databaseAccountOfferType": "Standard"
      }
    },
    {
      "type": "Microsoft.StreamAnalytics/streamingjobs",
      "name": "taxi-asa-job",
      "apiVersion": "2016-03-01",
      "location": "West US",
      "properties": {
        "sku": {
          "name": "Standard"
        },
        "eventsOutOfOrderPolicy": "Adjust",
        "outputErrorPolicy": "Stop",
        "eventsOutOfOrderMaxDelayInSeconds": 900,
        "eventsLateArrivalMaxDelayInSeconds": 1805,
        "dataLocale": "en-US",
        "compatibilityLevel": "1.1",
        "inputs": [
          {
            "name": "TaxiRide",
            "properties": {
              "type": "Stream",
              "datasource": {
                "type": "Microsoft.ServiceBus/EventHub",
                "properties": {
                  "eventHubName": "[variables('taxiRideEventHub')]",
                  "consumerGroupName": "[variables('taxiRideConsumerGroup')]",
                  "serviceBusNamespace": "[parameters('eventHubNamespace')]",
                  "sharedAccessPolicyName": "[variables('taxiRideSharedAccessPolicy')]",
                  "sharedAccessPolicyKey": "[listKeys(variables('taxiRideSharedAccessPolicyResourceId'), '2017-04-01').primaryKey]"
                }
              },
              "compression": {
                "type": "None"
              },
              "serialization": {
                "type": "Json",
                "properties": {
                  "encoding": "UTF8"
                }
              }
            }
          },
          {
            "name": "TaxiFare",
            "properties": {
              "type": "Stream",
              "datasource": {
                "type": "Microsoft.ServiceBus/EventHub",
                "properties": {
                  "eventHubName": "[variables('taxiFareEventHub')]",
                  "consumerGroupName": "[variables('taxiFareConsumerGroup')]",
                  "serviceBusNamespace": "[parameters('eventHubNamespace')]",
                  "sharedAccessPolicyName": "[variables('taxiFareSharedAccessPolicy')]",
                  "sharedAccessPolicyKey": "[listKeys(variables('taxiFareSharedAccessPolicyResourceId'), '2017-04-01').primaryKey]"
                }
              },
              "compression": {
                "type": "None"
              },
              "serialization": {
                "type": "Csv",
                "properties": {
                  "fieldDelimiter": ",",
                  "encoding": "UTF8"
                }
              }
            }
          }
        ],
        "outputs": [
          {
            "name": "TaxiDrain",
            "properties": {
              "datasource": {
                "type": "Microsoft.Storage/DocumentDB",
                "properties": {
                  "accountId": "[parameters('outputCosmosDatabaseAccount')]",
                  "accountKey": "[listKeys(resourceId('Microsoft.DocumentDB/databaseAccounts', parameters('outputCosmosDatabaseAccount')), '2015-04-08').primaryMasterKey]",
                  "database": "[parameters('outputCosmosDatabase')]",
                  "collectionNamePattern": "[parameters('outputCosmosDatabaseCollection')]",
                  "partitionKey": null,
                  "documentId": "WindowTime"
                }
              },
              "serialization": {
                "type": "Json",
                "properties": {
                  "format": "LineSeparated",
                  "encoding": "UTF8"
                }
              }
            }
          }
        ],
        "transformation": {
          "name": "Transformation",
          "properties": {
            "streamingUnits": 60,
            "query": "WITH\r\nStep1 AS (\r\n    SELECT PartitionId,\r\n           TRY_CAST(Medallion AS nvarchar(max)) AS Medallion,\r\n           TRY_CAST(HackLicense AS nvarchar(max)) AS HackLicense,\r\n           VendorId,\r\n           TRY_CAST(PickupTime AS datetime) AS PickupTime,\r\n           TripDistanceInMiles\r\n    FROM [TaxiRide] PARTITION BY PartitionId\r\n),\r\nStep2 AS (\r\n    SELECT PartitionId,\r\n           medallion AS Medallion,\r\n           hack_license AS HackLicense,\r\n           vendor_id AS VendorId,\r\n           TRY_CAST(pickup_datetime AS datetime) AS PickupTime,\r\n           tip_amount AS TipAmount\r\n    FROM [TaxiFare] PARTITION BY PartitionId\r\n),\r\nStep3 AS (\r\n  SELECT tr.PartitionId AS TaxiRidePartitionId,\r\n         tf.PartitionId AS TaxiFarePartitionId,\r\n         tr.Medallion,\r\n         tr.HackLicense,\r\n         tr.VendorId,\r\n         tr.PickupTime,\r\n         tr.TripDistanceInMiles,\r\n         tf.TipAmount\r\n    FROM [Step1] tr\r\n    JOIN [Step2] tf\r\n      ON tr.Medallion = tf.Medallion\r\n     AND tr.HackLicense = tf.HackLicense\r\n     AND tr.VendorId = tf.VendorId\r\n     AND tr.PickupTime = tf.PickupTime\r\n     AND tr.PartitionId = tf.PartitionId\r\n     AND DATEDIFF(minute, tr, tf) BETWEEN 0 AND 15\r\n)\r\n\r\nSELECT System.Timestamp AS WindowTime,\r\n       SUM(tr.TipAmount) / SUM(tr.TripDistanceInMiles) AS AverageTipPerMile\r\n  INTO [TaxiDrain]\r\n  FROM [Step3] tr\r\n  GROUP BY HoppingWindow(Duration(minute, 5), Hop(minute, 1))"
          }
        }
      },
      "dependsOn": [
        "[variables('taxiRideSharedAccessPolicyResourceId')]",
        "[variables('taxiFareSharedAccessPolicyResourceId')]",
        "[variables('taxiRideConsumerGroupResourceId')]",
        "[variables('taxiFareConsumerGroupResourceId')]",
        "[concat('Microsoft.DocumentDb/databaseAccounts/', parameters('outputCosmosDatabaseAccount'))]"
      ]
